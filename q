[32m+[m
[32m+[m[32m# See https://wiki.hyprland.org/Configuring/Keywords/[m
[32m+[m[32m$mainMod = SUPER # Sets "Windows" key as main modifier[m
[32m+[m
[32m+[m[32m# Example binds, see https://wiki.hyprland.org/Configuring/Binds/ for more[m
[32m+[m[32mbind = $mainMod, RETURN, exec, $terminal[m
[32m+[m[32mbind = $mainMod, Q, killactive,[m
[32m+[m[32mbind = $mainMod, M, exit,[m
[32m+[m[32mbind = $mainMod, E, exec, $fileManager[m
[32m+[m[32mbind = $mainMod, V, exec, clipman pick -t wofi[m
[32m+[m[32mbind = $mainMod, F, togglefloating,[m
[32m+[m[32mbind = $mainMod, SPACE, exec, $menu[m
[32m+[m[32mbind = $mainMod, P, pseudo, # dwindle[m
[32m+[m[32mbind = $mainMod, J, togglesplit, # dwindle[m
[32m+[m
[32m+[m[32m# Setup Hyprlocks[m
[32m+[m[32mbind = $mainMod, L, exec, hyprlock[m
[32m+[m
[32m+[m[32m# Setup screenshots[m
[32m+[m[32mbind = , PRINT, exec, hyprshot -m window[m
[32m+[m[32mbind = SHIFT, PRINT, exec, hyprshot -m region[m
[32m+[m
[32m+[m[32m# Move focus with mainMod + arrow keys[m
[32m+[m[32mbind = $mainMod, left, movefocus, l[m
[32m+[m[32mbind = $mainMod, up, movefocus, u[m
[32m+[m[32mbind = $mainMod, down, movefocus, d[m
[32m+[m[32mbind = $mainMod, right, movefocus, r[m
[32m+[m
[32m+[m[32m# Switch workspaces with mainMod + [0-9][m
[32m+[m[32mbind = $mainMod, ampersand, workspace, 1[m
[32m+[m[32mbind = $mainMod, eacute, workspace, 2[m
[32m+[m[32mbind = $mainMod, quotedbl, workspace, 3[m
[32m+[m[32mbind = $mainMod, apostrophe, workspace, 4[m
[32m+[m[32mbind = $mainMod, parenleft, workspace, 5[m
[32m+[m[32mbind = $mainMod, section, workspace, 6[m
[32m+[m[32mbind = $mainMod, egrave, workspace, 7[m
[32m+[m[32mbind = $mainMod, exclam, workspace, 8[m
[32m+[m[32mbind = $mainMod, ccedilla, workspace, 9[m
[32m+[m[32mbind = $mainMod, agrave, workspace, 10[m
[32m+[m
[32m+[m[32m# Move active window to a workspace with mainMod + SHIFT + [0-9][m
[32m+[m[32mbind = $mainMod SHIFT, ampersand, movetoworkspace, 1[m
[32m+[m[32mbind = $mainMod SHIFT, eacute, movetoworkspace, 2[m
