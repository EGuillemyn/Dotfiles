[32m+[m[32mgeneral {[m
[32m+[m[32m    lock_cmd = pidof hyprlock || hyprlock       # avoid starting multiple hyprlock instances.[m
[32m+[m[32m    before_sleep_cmd = loginctl lock-session    # lock before suspend.[m
[32m+[m[32m    after_sleep_cmd = hyprctl dispatch dpms on  # to avoid having to press a key twice to turn on the display.[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlistener {[m
[32m+[m[32m    timeout = 150                                # 2.5min.[m
[32m+[m[32m    on-timeout = brightnessctl -s set 10         # set monitor backlight to minimum, avoid 0 on OLED monitor.[m
[32m+[m[32m    on-resume = brightnessctl -r                 # monitor backlight restore.[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlistener {[m
[32m+[m[32m    timeout = 300                                 # 5min[m
[32m+[m[32m    on-timeout = loginctl lock-session            # lock screen when timeout has passed[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlistener {[m
[32m+[m[32m    timeout = 330                                 # 5.5min[m
[32m+[m[32m    on-timeout = hyprctl dispatch dpms off        # screen off when timeout has passed[m
[32m+[m[32m    on-resume = hyprctl dispatch dpms on          # screen on when activity is detected after timeout has fired.[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlistener {[m
[32m+[m[32m    timeout = 1800                                # 30min[m
[32m+[m[32m    on-timeout = systemctl suspend                # suspend pc[m
[32m+[m[32m}[m
